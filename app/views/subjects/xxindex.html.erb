
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Who</th>
      <th>Status</th>
      <th>Finalend</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= link_to subject.name, subject_tasks_path(subject.name) %></td>
        <td><%= subject.description %></td>
        <td><%= subject.who %></td>
        <td><%= subject.status %></td>
        <td><%= subject.finalend %></td>
        <td><%= link_to 'Show', subject %></td>
        <td><%= link_to 'Edit', edit_subject_path(subject) %></td>
        <td><%= link_to 'Destroy', subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subject', new_subject_path %>
